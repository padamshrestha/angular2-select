{
    "name": "@baumi/angular2-select",
    "version": "2.1.11",
    "description": "An implementation of material design select element for Angular 2",
    "scripts": {
        "typings-install": "$(npm bin)/typings install",
        "postinstall": "npm run typings-install",
        "build-dev": "$(npm bin)/webpack --inline --colors --progress --display-error-details --display-cached",
        "build-lib": "$(npm bin)/tsc -p src -d",
        "build": "npm run build-lib && npm run build-dev",
        "prepublish": "npm run build",
        "server": "$(npm bin)/webpack-dev-server --inline --colors --progress --display-error-details --display-cached --port 5000  --content-base examples/webpack-dev-server",
        "start": "npm run server"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/rbaumi/angular2-select.git"
    },
    "keywords": [
        "angular2",
        "select",
        "selectbox"
    ],
    "author": "Rafal Baumann",
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/rbaumi/angular2-select/issues"
    },
    "main": "angular2-select.js",
    "typings": "./angular2-select.d.ts",
    "homepage": "https://github.com/rbaumi/angular2-select",
    "devDependencies": {
        "angular2-template-loader": "0.5.0",
        "awesome-typescript-loader": "^2.2.4",
        "css-loader": "0.25.0",
        "raw-loader": "0.5.1",
        "to-string-loader": "1.1.5",
        "typescript": "2.0.3",
        "typings": "~1.4.0",
        "webpack": "1.13.2",
        "webpack-dev-server": "1.16.2",
        "webpack-merge": "0.14.1"
    },
    "dependencies": {
        "@angular/common": "^2.0.1",
        "@angular/compiler": "^2.0.1",
        "@angular/core": "^2.0.1",
        "@angular/forms": "^2.0.1",
        "@angular/http": "^2.0.1",
        "@angular/platform-browser": "^2.0.1",
        "@angular/platform-browser-dynamic": "^2.0.1",
        "@angular/platform-server": "^2.0.1",
        "@angular2-material/card": "^2.0.0-alpha.8-2",
        "@angular2-material/core": "^2.0.0-alpha.8-2",
        "@angular2-material/icon": "^2.0.0-alpha.8-2",
        "@angular2-material/input": "^2.0.0-alpha.8-2",
        "core-js": "^2.4.1",
        "ie-shim": "^0.1.0",
        "rxjs": "^5.0.0-beta.12",
        "zone.js": "^0.6.25"
    }
}
