{
    "name": "@baumi/angular2-select",
    "version": "2.0.8",
    "description": "An implementation of material design select element for Angular 2",
    "scripts": {
        "typings-install": "typings install",
        "postinstall": "npm run typings-install",
        "build-dev": "webpack --inline --colors --progress --display-error-details --display-cached",
        "build-lib": "tsc -p src",
        "build": "npm run build-lib && npm run build-dev",
        "prepublish": "npm run build",
        "server": "webpack-dev-server --inline --colors --progress --display-error-details --display-cached --port 5000  --content-base examples/webpack-dev-server",
        "start": "npm run server"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/rbaumi/angular2-select.git"
    },
    "keywords": [
        "angular2",
        "select",
        "selectbox"
    ],
    "author": "Rafal Baumann",
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/rbaumi/angular2-select/issues"
    },
    "main": "angular2-select.js",
    "typings": "./angular2-select.d.ts",
    "homepage": "https://github.com/rbaumi/angular2-select",
    "devDependencies": {
        "angular2-template-loader": "^0.4.0",
        "awesome-typescript-loader": "^1.1.1",
        "css-loader": "^0.23.1",
        "raw-loader": "^0.5.1",
        "to-string-loader": "^1.1.4",
        "typescript": "^2.0.2",
        "typings": "~1.0.3",
        "webpack": "^1.12.9",
        "webpack-dev-server": "^1.14.0",
        "webpack-merge": "^0.8.4"
    },
    "dependencies": {
        "@angular/common": "2.0.0-rc.6",
        "@angular/compiler": "2.0.0-rc.6",
        "@angular/core": "2.0.0-rc.6",
        "@angular/forms": "2.0.0-rc.6",
        "@angular/http": "2.0.0-rc.6",
        "@angular/platform-browser": "2.0.0-rc.6",
        "@angular/platform-browser-dynamic": "2.0.0-rc.6",
        "@angular/platform-server": "2.0.0-rc.6",
        "@angular2-material/card": "2.0.0-alpha.8-1",
        "@angular2-material/core": "2.0.0-alpha.8-1",
        "@angular2-material/icon": "2.0.0-alpha.8-1",
        "@angular2-material/input": "2.0.0-alpha.8-1",
        "core-js": "^2.4.0",
        "ie-shim": "^0.1.0",
        "rxjs": "5.0.0-beta.11",
        "zone.js": "~0.6.17"
    }
}
